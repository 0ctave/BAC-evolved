{
	"name": "sveltekit",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev --port 3000",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint . && prettier --check .",
		"lint:fix": "eslint --fix \"src/**/*.{js,ts,svelte}\" && prettier --write .",
		"format": "prettier --write .",
		"generate:types": "tsx ./src/lib/directus/generateDirectusTypes.ts"
	},
	"devDependencies": {
		"@eslint/compat": "2.0.1",
		"@eslint/js": "9.39.2",
		"@sveltejs/enhanced-img": "0.9.3",
		"@sveltejs/kit": "2.50.1",
		"@sveltejs/vite-plugin-svelte": "6.2.4",
		"@tailwindcss/postcss": "4.1.18",
		"@types/node": "25.0.10",
		"autoprefixer": "10.4.23",
		"bits-ui": "2.15.4",
		"clsx": "2.1.1",
		"directus-sdk-typegen": "0.2.1",
		"eslint": "9.39.2",
		"eslint-config-prettier": "10.1.8",
		"eslint-plugin-svelte": "3.14.0",
		"formsnap": "2.0.1",
		"globals": "17.1.0",
		"prettier": "3.8.1",
		"prettier-plugin-svelte": "3.4.1",
		"prettier-plugin-tailwindcss": "0.7.2",
		"svelte": "5.48.0",
		"svelte-check": "4.3.5",
		"sveltekit-superforms": "2.29.1",
		"tailwind-merge": "3.4.0",
		"tailwind-variants": "3.2.2",
		"tailwindcss": "4.1.18",
		"tailwindcss-animate": "1.0.7",
		"tsx": "4.21.0",
		"typescript": "5.9.3",
		"typescript-eslint": "8.53.1",
		"vite": "7.3.1"
	},
	"dependencies": {
		"@directus/sdk": "21.0.0",
		"@directus/visual-editing": "1.1.0",
		"@internationalized/date": "3.10.1",
		"@lucide/svelte": "0.563.1",
		"@sveltejs/adapter-node": "5.5.2",
		"@tailwindcss/container-queries": "0.1.1",
		"@tailwindcss/forms": "0.5.11",
		"@tailwindcss/typography": "0.5.19",
		"class-variance-authority": "0.7.1",
		"dotenv": "17.2.3",
		"intl-tel-input": "26.0.9",
		"lodash-es": "4.17.23",
		"mode-watcher": "1.1.0",
		"p-queue": "9.1.0",
		"svelte-tel-input": "3.6.1",
		"zod": "4.3.6"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild",
			"sharp"
		]
	}
}
